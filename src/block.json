{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "markonikolas/funky-headline",
	"version": "0.0.1",
	"title": "Funky Headline",
	"category": "text",
	"icon": "carrot",
	"description": "A funky headline block with an eye-catching look :)",
	"example": {
		"kicker": {
			"type": "string",
			"default": "Real Science"
		},
		"kickerColor": {
			"type": "string",
			"default": "#fff"
		},
		"kickerBackgroundColor": {
			"type": "string",
			"default": "#5502dd"
		},
		"headline": {
			"type": "string",
			"default": "Colorful Headlines Increase Reader Engagement by 400%"
		},
		"headlineColor": {
			"type": "string",
			"default": "#000"
		},
		"subdeck": {
			"type": "string",
			"default": "Boffins at the University of Science discover the secrets of colorful headlines and how they hypnotize anyone who reads them."
		},
		"subdeckColor": {
			"type": "string",
			"default": "#333"
		}
	},
	"supports": {
		"html": false,
		"spacing": {
			"padding": true,
			"margin": true
		},
		"color": {
			"background": true,
			"text": false
		}
	},
	"attributes": {
		"kicker": {
			"type": "string",
			"source": "html",
			"selector": "span",
			"default": "Real Science"
		},
		"kickerColor": {
			"type": "string",
			"default": "#fff"
		},
		"kickerBackgroundColor": {
			"type": "string",
			"default": "#5502dd"
		},
		"headline": {
			"type": "string",
			"source": "html",
			"selector": "h1",
			"default": "Colorful Headlines Increase Reader Engagement by 400%"
		},
		"headlineColor": {
			"type": "string",
			"default": "#ff008c"
		},
		"subdeck": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": "Boffins at the University of Science discover the secrets of colorful headlines and how they hypnotize anyone who reads them."
		},
		"subdeckColor": {
			"type": "string",
			"default": "#828282"
		}
	},
	"textdomain": "funky-headline",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
